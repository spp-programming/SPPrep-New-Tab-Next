{
    "name": "SPP New Tab - 2024-25",
    "version": "4.0",
    "description": "A full rewrite of SPP New Tab.",
    "homepage_url": "https://github.com/spp-programming/SPPrep-New-Tab-Next",
    "icons": {
      "16": "img/icons/extension/16.png",
      "32": "img/icons/extension/32.png",
      "48": "img/icons/extension/48.png",
      "128": "img/icons/extension/128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage",
      "unlimitedStorage"
    ],
    "options_page": "settings.html",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "manifest_version": 3
}